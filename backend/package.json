{
	"name": "backend",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=14.15.0"
	},
	"scripts": {
		"db:setup": "sequelize db:drop --env dev && sequelize db:create --env dev && npm run db:migrate",
		"db:migrate": "sequelize db:migrate --env dev",
		"db:migrate:undo": "sequelize db:migrate:undo --env dev",
		"dev": "sls offline start -s dev",
		"deploy": "sls deploy -s prod -v",
		"test:watch": "jest --watch",
		"test:all": "jest --coverage",
		"lint": "eslint . --ext .js,.ts",
		"lint:fix": "eslint . --ext .js,.ts --fix"
	},
	"dependencies": {
		"@aws-sdk/client-apigatewaymanagementapi": "^3.12.0",
		"@middy/core": "^1.5.2",
		"@middy/http-json-body-parser": "^1.5.2",
		"@middy/http-response-serializer": "^2.0.0",
		"pg": "^8.6.0",
		"pg-hstore": "^2.3.3",
		"sequelize": "^6.6.2"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.13.15",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.3.1",
		"jest": "^26.6.3",
		"prettier": "^2.2.1",
		"sequelize-cli": "^6.2.0",
		"serverless": "^2.23.0",
		"serverless-domain-manager": "^5.1.0",
		"serverless-offline": "^6.9.0",
		"serverless-prune-plugin": "^1.4.4",
		"serverless-webpack": "^5.3.5",
		"webpack": "^5.20.2",
		"webpack-node-externals": "^2.5.2"
	}
}
